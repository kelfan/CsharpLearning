<Application x:Class="csharpDB.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:lc="clr-namespace:csharpDB.Data"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <lc:FreData x:Key="fredata"/>
        <ObjectDataProvider x:Key="allFredata"
                            ObjectInstance="{StaticResource fredata}"
                            MethodName="getData">
            
        </ObjectDataProvider>
    </Application.Resources>
</Application>
